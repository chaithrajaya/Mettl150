import java.util.Scanner;
class Mettl43{
public static void main(String[] args){
Scanner sc =new Scanner(System.in);
System.out.print("Enter n:");
int n = sc.nextInt();
System.out.println();
for(int i=1;i<=n;i++){

for(int j=1,p=1;j<=i;j++,p++){
System.out.print(p);
}

System.out.println();
}
}
}



OUTPUT:
Enter n:5

1
12
123
1234
12345
